	skos:notation "7.3" ;
	skos:prefLabel "Водный транспорт"@ru .
# 
# https://w3id.org/cityont/kvrizy#воздушный_транспорт

:воздушный_транспорт a owl:NamedIndividual , skos:Concept , cityont:E05_LandUseType ;
	skos:broader :транспорт ;
